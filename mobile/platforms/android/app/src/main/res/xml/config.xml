<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ankur.tarana" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Tarana</name>
    <description>
        An online radio app
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Ankur's creation
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="res/drawable-mdpi/tarana_logo.png" />
    <icon density="hdpi" src="res/drawable-hdpi/tarana_logo.png" />
    <icon density="xhdpi" src="res/drawable-xhdpi/tarana_logo.png" />
    <icon density="xxhdpi" src="res/drawable-xxhdpi/tarana_logo.png" />
    <icon density="xxxhdpi" src="res/drawable-xxxhdpi/tarana_logo.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
